{
  "manifest_version": 2,
  "name": "Remove Podcasts from Spotify",
  "version": "1.3",
  "description": "Removes most podcast references in the Spotify web client",
  "icons": {
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.spotify.com/*"
      ],
      "js": [
        "index.js"
      ]
    }
  ]
}